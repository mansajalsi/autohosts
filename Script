KALITRICKS = 1
gg.setVisible(false)
SAJAL = 1
function KALITRICKS()
menu = gg.multiChoice({
	"🎯 🇼‌🇭‌ & 🇨‌🇭‌",
	"🎯 🇱‌🇪‌🇸‌🇸‌ 🇷‌🇪‌🇨‌🇴‌🇮‌🇱‌‌",
	"🎯 🇸‌🇦‌🇫‌🇪‌ 🇭‌🇪‌🇦‌🇩‌🇸‌🇭‌🇴‌🇹‌",
	"🎯 🇮‌🇵‌🇦‌🇩‌ 🇻‌🇮‌🇪‌🇼‌",
	"🎯 ‌🇱‌🇴‌🇬‌🇸‌ 🇨‌🇱‌🇪‌🇦‌🇳‌",
	"☠️ 🇪‌🇽‌🇮‌🇹‌"
	},nil, "🎀 Made By 🇸‌🇦‌🇯‌🇦‌🇱‌ 🎀")
	if menu == nil then else
	if menu[1] == true then on() end
	if menu[2] == true then lr() end
	if menu[3] == true then hss() end
	if menu[4] == true then aimb() end
	if menu[5] == true then off() end
	if menu[6] == true then exit() end
	end
    SAJAL = -1
	end
	
	
	function exit()
	print("🎀 Script Made By 🇸‌🇦‌🇯‌🇦‌🇱‌ 🎀")
	print("🎀 Values By 🇸‌🇦‌🇯‌🇦‌🇱‌")
	os.exit()
	end
	
	
	
function on()
gg.clearResults()
gg.setRanges(gg.REGION_BAD)
gg.searchNumber("2;1.8947657e-40;5.8013756e-42", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(10)
gg.editAll("120", gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber("2.718519e-43;3.7615819e-37;2;-1;1;-127", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(10)
gg.editAll("120", gg.TYPE_FLOAT)
gg.clearResults()
gg.setRanges(gg.REGION_BAD)
gg.searchNumber("8200;96", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("8200", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("098", -1, gg.TYPE_DWORD, gg.SIGN_EQUAL, 0, -1)
gg.getResults(999)
gg.editAll("8198", gg.TYPE_DWORD)
gg.clearResults()
gg.toast("Wallhack + Colour ")
gg.setVisible(false) 
end

function off()
