end
end
PUBGMH = -1
end

function ipad()
gg.clearResults()
  gg.setRanges(gg.REGION_ANONYMOUS)
  gg.searchNumber("2.8025969e-45;220;25;178;15;100", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.searchNumber("220", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.getResults(100)
  gg.editAll("440", gg.TYPE_FLOAT)
  gg.toast("Ipad View")
end

function sison()
gg.setRanges(32)
gg.clearResults()
gg.searchNumber("1,100,159,584", 4, false, 536870912, 0, -1)
gg.getResults(100)
gg.editAll("1,135,199,584", 4)
gg.clearResults()
gg.toast("SITSCOOP ON")
end

function sisoff() 
gg.setRanges(32)
gg.clearResults()
gg.searchNumber("1,135,199,584", 4, false, 536870912, 0, -1)
gg.getResults(100)
gg.editAll("1,100,159,584", 4)
gg.clearResults()
gg.toast("SITSCOOP OFF")
end

function highjump()
gg.clearResults()
gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("1;35;443;55;0.57357645035", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("1", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("34", -1, gg.TYPE_FLOAT, gg.SIGN_EQUAL, 0, -1)
gg.getResults(100)
gg.editAll("3", gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber("443", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("3C", -1, gg.TYPE_FLOAT, gg.SIGN_EQUAL, 0, -1)
gg.getResults(100)
gg.editAll("2500", gg.TYPE_FLOAT)
gg.clearResults()
gg.toast("HIGHJUMP ACTIVATED")
end

